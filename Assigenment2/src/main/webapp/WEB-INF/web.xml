 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Todo_insert</servlet-name>
  	<display-name>Todo_insert</display-name>
  	<description></description>
  	<servlet-class>com.mycompany.Todo_insert</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Todo_update</servlet-name>
  	<display-name>Todo_update</display-name>
  	<description></description>
  	<servlet-class>com.mycompany.Todo_update</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Todo_insert</servlet-name>
  	<url-pattern></url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Todo_update</servlet-name>
  	<url-pattern>/Todo_update</url-pattern>
  </servlet-mapping>
</web-app>
